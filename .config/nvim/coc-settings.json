{
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    }
  },
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "cs",
    "graphql",
    "go",
    "javascript",
    "javascriptreact",
    "lua",
    "typescript",
    "typescriptreact",
    "svelte",
    "json",
    "scss"
  ],
  "svelte.enable-ts-plugin": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.diagnostics.globals": ["vim"],
  "Lua.telemetry.enable": false,
  "Lua.workspace.library": [
    "/opt/homebrew/Cellar/neovim/0.7.0/share/nvim/runtime",
    "/opt/homebrew/Cellar/neovim/0.7.0/share/nvim/runtime/lua",
    "/opt/homebrew/Cellar/neovim/0.7.0/share/nvim/runtime/lua/vim",
    "/opt/homebrew/Cellar/neovim/0.7.0/share/nvim/runtime/lua/vim/lsp"
  ],
  "eslint.autoFixOnSave": true
}
